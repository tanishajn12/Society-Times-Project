<%- include('./partials/header') %>
<%- include('./partials/navbar') %>

<body>
    <!-- ======= Header ======= -->
    <header id="header" class="fixed-top d-flex align-items-center">
      <div class="container d-flex align-items-center">
  
        <div class="logo me-auto">
          <!-- <h5>Society Times</h5> -->
          <a href="index.html"><img src="./assets/logo2.png" alt="" class="img-fluid"></a>
        </div>
  
        <nav id="navbar" class="navbar order-last order-lg-0">
          
          <ul>
            <li><a class="nav-link scrollto active" href="/">Home</a></li>
            <li class="dropdown"><a href="#"><span>About</span> <i class="bi bi-chevron-down"></i></a>
              <ul>
                <li><a class="nav-link scrollto" href="#about">About Us</a></li>
                <li><a class="nav-link scrollto" href="#testimonials">Testimonials</a></li>
                <li><a class="nav-link scrollto" href="#services">Services</a></li>
                
              </ul>
            </li>
            
            <li><a class="nav-link scrollto " href="/events">Events</a></li>
            <li><a class="nav-link scrollto " href="/societies">Societies</a></li>
            <li><a class="nav-link scrollto" href="/contact">Contact</a></li>
          </ul>
          <div class="d-flex" role="search">
            <% if (!currentUser) { %>
              <a class="mx-2" href="/login"><button class="btn btn-outline-info" type="submit">Login</button></a>
              <a class="mx-2" href="/register"><button class="btn btn-outline-info" type="submit">Signup</button></a>
            <% } else { %>

              
              <a href="#" style="font-size:17px;; font-family:sans-serif ; color:black;" class="nav-link text-capitalize">Hello <%= currentUser.name %></a>

              <a href="/profile"><i class="fa fa-user-circle" style="font-size: 28px; color: #009cea; margin-right:25px"></i></a>
              <a class="mx-2" href="/logout"><button class="btn btn-outline-info" type="submit">Logout</button></a>
            <% } %>
          </div>
          <i class="bi bi-list mobile-nav-toggle"></i>
  
        </nav><!-- .navbar -->
  
  
      </div>
    </header><!-- End Header -->

  <!-- ======= Hero Section ======= -->
  <section id="hero">

    <div class="container">
      <div class="row">
        <div class="col-lg-6 pt-5 pt-lg-0 order-2 order-lg-1 d-flex flex-column justify-content-center" data-aos="fade-up">
          <div>
            <h1>Welcome To </h1>
            <h1>Society Times</h1>
            <h2>Your daily bulletin for the latest news and stories about Society Events</h2>
            <a href="#about" class="btn-get-started scrollto">Read more</a>
          </div>
        </div>
        <div class="col-lg-6 order-1 order-lg-2 hero-img" data-aos="fade-left">
          <img src="/assets/graphics/Work chat-amico.svg" class="img-fluid" alt="">
        </div>
      </div>
    </div>

  </section><!-- End Hero -->

  <main id="main">

    <!-- About Section -->
    <section id="about" class="about content-section">
      <div class="container">
          <div class="row">
              <div class="col-lg-6" data-aos="zoom-in">
                  <img src="/assets/graphics/Events-pana.svg" class="img-fluid" alt="About Society Times">
              </div>
              <div class="col-lg-6 d-flex flex-column justify-content-center" data-aos="fade-left">
                  <div class="content pt-4 pt-lg-0">
                      <h3>Why Society Times ?</h3>
                      <p class="fst-italic">
                        Say no to missing out on exciting events! Society Times is here to revolutionize how you manage and participate in society events. Whether you're an administrator or a student, our platform ensures a seamless and engaging experience.
                      </p>
                      <ul>
                          <li><i class="bi bi-check-circle"></i> Facilitates both administrators and students in managing society activities.</li>
                          <li><i class="bi bi-check-circle"></i> Enhances engagement and management of society events.</li>
                          <li><i class="bi bi-check-circle"></i> Makes it easier for administrators to organize and for students to participate actively.</li>
                      </ul>
                  </div>
              </div>
          </div>
      </div>
  </section><!-- End About Section -->

    <!-- ======= Features Section ======= -->
    <section id="features" class="features">
      <div class="container">
        <h2>Features</h2>

        <div class="row">
          <div class="col-lg-6 mt-2 mb-tg-0 order-2 order-lg-1">
            <ul class="nav nav-tabs flex-column">
              <li class="nav-item" data-aos="fade-up">
                <a class="nav-link active show" data-bs-toggle="tab" href="#tab-1">
                  <h4><strong>Effortless Event Management:</strong></h4>
                  <p>Society Administrators can easily create, update, and manage events, ensuring everything runs smoothly.</p>
          
                </a>
              </li>
              <li class="nav-item mt-2" data-aos="fade-up" data-aos-delay="100">
                <a class="nav-link" data-bs-toggle="tab" href="#tab-2">
                  <h4><strong>Discover Societies:</strong></h4>
                  <p>Students can explore various societies, find events that interest them, and get involved with just a few clicks.</p>
                </a>
              </li>
              <li class="nav-item mt-2" data-aos="fade-up" data-aos-delay="200">
                <a class="nav-link" data-bs-toggle="tab" href="#tab-3">
                  <h4><strong>Stay Updated:</strong></h4>
                  <p>Never miss an event with timely information and easy access to event details.</p>
                </a>
              </li>
              <li class="nav-item mt-2" data-aos="fade-up" data-aos-delay="300">
                <a class="nav-link" data-bs-toggle="tab" href="#tab-4">
                  <h4> <strong>Engage and Review:</strong></h4>
                  <p>Like your favorite events, add them to your wishlist, and share your experiences by leaving reviews.</p>
                </a>
              </li>
            </ul>
          </div>
          <div class="col-lg-6 order-1 order-lg-2" data-aos="zoom-in">
            <div class="tab-content">
              <div class="tab-pane active show" id="tab-1">
                <figure>
                  <img src="/assets/graphics/analytics.png" alt="" class="img-fluid">
                </figure>
              </div>
              <div class="tab-pane" id="tab-2">
                <figure>
                  <img src="/assets/graphics/societies.png" alt="" class="img-fluid">
                </figure>
              </div>
              <div class="tab-pane" id="tab-3">
                <figure>
                  <img src="/assets/graphics/events.png" alt="" class="img-fluid">
                </figure>
              </div>
              <div class="tab-pane" id="tab-4">
                <figure>
                  <img src="/assets/graphics/review.png" alt="" class="img-fluid">
                </figure>
              </div>
            </div>
          </div>
        </div>

      </div>
    </section><!-- End Features Section -->

    <!-- ======= Services Section ======= -->
    <section id="services" class="services section-bg">
      <div class="container">

        <div class="section-title" data-aos="fade-up">
          <h2 style="color:black">Our Services</h2>
        </div>

        <div class="row">
          <div class="col-md-6 col-lg-3 d-flex align-items-stretch mb-5 mb-lg-0" data-aos="zoom-in">
            <div class="icon-box icon-box-pink">
              <div class="icon"><i class="bx bx-chat"></i></div>
              <h4 class="title"><a href="">Streamlined Registration</a></h4>
              <p class="description">Helps students log in and register for events without refilling their information each time.</p>
            </div>
          </div>

          <div class="col-md-6 col-lg-3 d-flex align-items-stretch mb-5 mb-lg-0" data-aos="zoom-in" data-aos-delay="100">
            <div class="icon-box icon-box-cyan">
              <div class="icon"><i class="bx bx-file"></i></div>
              <h4 class="title"><a href="">Event Analytics</a></h4>
              <p class="description">Admins can view detailed analytics and statistics on event participation, helping them understand engagement and improve future events.</p>
            </div>
          </div>

          <div class="col-md-6 col-lg-3 d-flex align-items-stretch mb-5 mb-lg-0" data-aos="zoom-in" data-aos-delay="200">
            <div class="icon-box icon-box-green">
              <div class="icon"><i class="bx bx-like"></i></div>
              <h4 class="title"><a href="">Event Feedback</a></h4>
              <p class="description">Students reviews provides valuable feedback to organizers for future events.</p>
            </div>
          </div>

          <div class="col-md-6 col-lg-3 d-flex align-items-stretch mb-5 mb-lg-0" data-aos="zoom-in" data-aos-delay="300">
            <div class="icon-box icon-box-blue">
              <div class="icon"><i class="bx bx-world"></i></div>
              <h4 class="title"><a href="">Society Management</a></h4>
              <p class="description">Admins can manage their societies by adding or updating information about them, making it easier for students to explore and join societies that interest them.</p>
            </div>
          </div>

        </div>

      </div>
    </section><!-- End Services Section -->

